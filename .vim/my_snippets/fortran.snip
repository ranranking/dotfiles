snippet id
abbr integer dimension
options head
	integer, dimension(${1::})${2:, allocatable} :: ${3}

snippet rd
abbr real dimension
options head
	real, dimension(${1::})${2:, allocatable} :: ${3}

snippet cel
abbr CELL
